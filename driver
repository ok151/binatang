public class Driver {
    public static void main(String[] args) {
        jerapah k = new jerapah("grief", 4, "tidak bersuara", "coklat bintik bintik");
        k.displayData();
        System.out.println();
        tikus g = new tikus("jerry", 4, "cit cit", "Abu-abu");
        g.displayData();
        System.out.println();
        singa b = new singa("leo", 4, "rawr rawr", "Coklat");
        b.displayData();
        System.out.println();

    }
}
